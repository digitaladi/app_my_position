{
  "expo": {
    "name": "position_translation",
    "slug": "position_translation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/carte.png", 
  
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/carte.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/carte.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/carte.png",
      "output": "server",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
